{
  "version": 2,
  "name": "Dostoevsky",
  "builds": [
    {
      "src": "index.py",
      "use": "@now/python@canary",
      "config": { "maxLambdaSize": "25mb" }
    },{
      "src": "now_build_staticfiles.sh",
      "use": "@now/static-build"
    }
  ],
  "routes": [{"src": "/(.*)", "dest": "/"}
  ]
}
